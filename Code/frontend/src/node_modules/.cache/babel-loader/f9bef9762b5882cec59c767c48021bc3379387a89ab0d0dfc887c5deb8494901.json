{"ast":null,"code":"import userMixins from '../mixins/userMixin';\nimport NavBar from '../components/NavBar.vue';\nexport default {\n  mixins: [userMixins],\n  data() {\n    return {\n      sectionName: \"\",\n      books: []\n    };\n  },\n  mounted() {\n    const sectionId = this.$route.params.id;\n    this.getBooks(sectionId);\n  },\n  methods: {\n    getBooks(sectionId) {\n      fetch(`http://127.0.0.1:5000/section/${sectionId}`, {\n        method: 'GET',\n        headers: {\n          'Content-Type': 'application/json',\n          'Authorization': 'Bearer ' + localStorage.getItem('access_token')\n        }\n      }).then(response => {\n        if (!response.ok) {\n          throw new Error(`HTTP error: ${response.status}`);\n        }\n        return response.json();\n      }).then(data => {\n        this.sectionName = data.section.name;\n      }).catch(error => {\n        console.error('Error:', error);\n      });\n      fetch(`http://127.0.0.1:5000/book/section/${sectionId}`, {\n        method: 'GET',\n        headers: {\n          'Content-Type': 'application/json',\n          'Authorization': 'Bearer ' + localStorage.getItem('access_token')\n        }\n      }).then(response => {\n        if (!response.ok) {\n          throw new Error(`HTTP error: ${response.status}`);\n        }\n        return response.json();\n      }).then(data => {\n        this.books = data.books;\n      }).catch(error => {\n        console.error('Error:', error);\n      });\n    },\n    createBook(sectionId) {\n      this.$router.push({\n        name: \"create-book\",\n        params: {\n          id: sectionId\n        }\n      });\n    },\n    updateBook(bookId) {\n      this.$router.push({\n        name: 'update-book',\n        params: {\n          id: bookId\n        }\n      });\n    },\n    deleteBook(bookId) {\n      const sectionId = this.$route.params.id;\n      fetch(`http://127.0.0.1:5000/book/${bookId}`, {\n        method: 'DELETE',\n        headers: {\n          'Content-Type': 'application/json',\n          'Authorization': 'Bearer ' + localStorage.getItem('access_token')\n        }\n      }).then(response => {\n        if (!response.ok) {\n          throw new Error(`HTTP error! Status: ${response.status}`);\n        }\n        this.getBooks(sectionId);\n      }).catch(error => {\n        console.error(`Error deleting book with ID ${bookId}:`, error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["userMixins","NavBar","mixins","data","sectionName","books","mounted","sectionId","$route","params","id","getBooks","methods","fetch","method","headers","localStorage","getItem","then","response","ok","Error","status","json","section","name","catch","error","console","createBook","$router","push","updateBook","bookId","deleteBook"],"sources":["C:\\Users\\rchan\\Desktop\\uplagarize\\Code\\frontend\\src\\views\\BookSectionpage.vue"],"sourcesContent":["<template>\r\n  <div class=\"books-section\">\r\n    <NavBar />\r\n    <div class=\"container-fluid mt-5\">\r\n      <h2 class=\"text-center mb-4\">\r\n        Books in {{ sectionName }} section\r\n        <button v-if=\"this.isLibrarian\" class=\"btn btn-outline-primary ml-3\" @click=\"createBook(sectionId)\">Create New Book</button>\r\n      </h2>\r\n      <div class=\"custom-table-responsive\">\r\n        <table class=\"custom-table custom-table-bordered custom-table-hover\">\r\n          <thead class=\"custom-table-dark\">\r\n            <tr>\r\n              <th>ID</th>\r\n              <th>Name</th>\r\n              <th>Content</th>\r\n              <th>Authors</th>\r\n              <th>Book Link</th>\r\n              <th>Action(s)</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"book in books\" :key=\"book.id\">\r\n              <td>{{ book.id }}</td>\r\n              <td>{{ book.name }}</td>\r\n              <td>{{ book.content }}</td>\r\n              <td>{{ book.authors }}</td>\r\n              <td><a :href=\"book.book_link\" target=\"_blank\">Link</a></td>\r\n              <td>\r\n                <div class=\"custom-btn-group\" role=\"group\">\r\n                  <button v-if=\"this.isLibrarian\" class=\"btn btn-outline-primary\" @click=\"updateBook(book.id)\">Update</button>\r\n                  <button v-if=\"this.isLibrarian\" class=\"btn btn-outline-danger\" @click=\"deleteBook(book.id)\">Delete</button>\r\n                </div>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport userMixins from '../mixins/userMixin';\r\nimport NavBar from '../components/NavBar.vue';\r\n\r\nexport default {\r\n  mixins: [userMixins],\r\n  data() {\r\n    return {\r\n      sectionName: \"\",\r\n      books: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    const sectionId = this.$route.params.id;\r\n    this.getBooks(sectionId);\r\n  },\r\n  methods: {\r\n    getBooks(sectionId) {\r\n      fetch(`http://127.0.0.1:5000/section/${sectionId}`, {\r\n        method: 'GET',\r\n        headers: {\r\n          'Content-Type': 'application/json',\r\n          'Authorization': 'Bearer ' + localStorage.getItem('access_token'),\r\n        }\r\n      })\r\n      .then(response => {\r\n        if (!response.ok) {\r\n          throw new Error(`HTTP error: ${response.status}`);\r\n        }\r\n        return response.json();\r\n      })\r\n      .then(data => {\r\n        this.sectionName = data.section.name;\r\n      })\r\n      .catch(error => {\r\n        console.error('Error:', error);\r\n      });\r\n\r\n      fetch(`http://127.0.0.1:5000/book/section/${sectionId}`, {\r\n        method: 'GET',\r\n        headers: {\r\n          'Content-Type': 'application/json',\r\n          'Authorization': 'Bearer ' + localStorage.getItem('access_token'),\r\n        }\r\n      })\r\n      .then(response => {\r\n        if (!response.ok) {\r\n          throw new Error(`HTTP error: ${response.status}`);\r\n        }\r\n        return response.json();\r\n      })\r\n      .then(data => {\r\n        this.books = data.books;\r\n      })\r\n      .catch(error => {\r\n        console.error('Error:', error);\r\n      });\r\n    },\r\n    createBook(sectionId) {\r\n      this.$router.push({ name: \"create-book\", params: { id: sectionId } });\r\n    },\r\n    updateBook(bookId) {\r\n      this.$router.push({ name: 'update-book', params: { id: bookId } });\r\n    },\r\n    deleteBook(bookId) {\r\n      const sectionId = this.$route.params.id;\r\n      fetch(`http://127.0.0.1:5000/book/${bookId}`, {\r\n        method: 'DELETE',\r\n        headers: {\r\n          'Content-Type': 'application/json',\r\n          'Authorization': 'Bearer ' + localStorage.getItem('access_token'),\r\n        }\r\n      })\r\n      .then(response => {\r\n        if (!response.ok) {\r\n          throw new Error(`HTTP error! Status: ${response.status}`);\r\n        }\r\n        this.getBooks(sectionId);\r\n      })\r\n      .catch(error => {\r\n        console.error(`Error deleting book with ID ${bookId}:`, error);\r\n      });\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.books-section {\r\n  font-family: 'Roboto', sans-serif;\r\n  background-color: #f7fafc;\r\n  min-height: 100vh;\r\n  padding: 2rem;\r\n}\r\n\r\nh2 {\r\n  font-size: 1.75rem;\r\n  color: #2d3748;\r\n  font-weight: 700;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n\r\n.custom-table {\r\n  border-radius: 8px;\r\n  background: #ffffff;\r\n}\r\n\r\n.custom-table-bordered {\r\n  border: 1px solid #dee2e6;\r\n}\r\n\r\n.custom-table-hover tbody tr:hover {\r\n  background-color: #f1f5f9;\r\n}\r\n\r\n.custom-table thead {\r\n  background-color: #343a40;\r\n  color: #ffffff;\r\n}\r\n\r\n.custom-table th, .custom-table td {\r\n  padding: 1rem;\r\n  vertical-align: middle;\r\n}\r\n\r\n.custom-table th {\r\n  font-weight: 600;\r\n}\r\n\r\n.custom-table td {\r\n  font-size: 0.875rem;\r\n}\r\n\r\