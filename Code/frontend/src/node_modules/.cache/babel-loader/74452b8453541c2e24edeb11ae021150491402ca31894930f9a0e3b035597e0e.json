{"ast":null,"code":"import userMixins from '../mixins/userMixin';\nimport NavBar from '../components/NavBar.vue';\nexport default {\n  mixins: [userMixins],\n  data() {\n    return {\n      books: [],\n      user: {}\n    };\n  },\n  mounted() {\n    this.getAllBooks();\n  },\n  methods: {\n    getAllBooks() {\n      fetch(\"http://127.0.0.1:5000/book\", {\n        method: 'GET',\n        headers: {\n          'Content-Type': 'application/json',\n          'Authorization': 'Bearer ' + localStorage.getItem('access_token')\n        }\n      }).then(response => {\n        if (!response.ok) {\n          throw new Error(`HTTP error: ${response.status}`);\n        }\n        return response.json();\n      }).then(data => {\n        this.books = data.books;\n      }).catch(error => {\n        console.error('Error:', error);\n      });\n    },\n    requestBook(book_id) {\n      fetch(`http://127.0.0.1:5000/bookrequest/${this.userid}/${book_id}`, {\n        method: 'POST',\n        headers: {\n          'Authorization': 'Bearer ' + localStorage.getItem('access_token'),\n          'Content-Type': 'application/json'\n        }\n      }).then(response => {\n        if (!response.ok) {\n          throw new Error(\"Book request is not completed \");\n        }\n        return response.json();\n      }).then(data => {\n        alert(data.message);\n      }).catch(error => {\n        console.error(\"Error:\", error);\n        alert(\"An error occurred while requesting the book.\");\n      });\n    }\n  }\n};","map":{"version":3,"names":["userMixins","NavBar","mixins","data","books","user","mounted","getAllBooks","methods","fetch","method","headers","localStorage","getItem","then","response","ok","Error","status","json","catch","error","console","requestBook","book_id","userid","alert","message"],"sources":["C:\\Users\\rchan\\Desktop\\uplagarize\\Code\\frontend\\src\\views\\Bookspage.vue"],"sourcesContent":["<template>\r\n  <div class=\"books-page\">\r\n    <NavBar />\r\n    <div class=\"container-fluid mt-5\">\r\n      <h2 class=\"text-center mb-4\">All Books</h2>\r\n      <div class=\"books mt-5\">\r\n        <div class=\"table-responsive\">\r\n          <table class=\"table table-bordered table-hover\">\r\n            <thead class=\"table-dark\">\r\n              <tr>\r\n                <th>ID</th>\r\n                <th>Name</th>\r\n                <th>Content</th>\r\n                <th>Authors</th>\r\n                <th>Book Link</th>\r\n                <th>Action(s)</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"book in books\" :key=\"book.id\">\r\n                <td>{{ book.id }}</td>\r\n                <td>{{ book.name }}</td>\r\n                <td>{{ book.content }}</td>\r\n                <td>{{ book.authors }}</td>\r\n                <td><a :href=\"book.book_link\" target=\"_blank\">Link</a></td>\r\n                <td>\r\n                  <div class=\"btn-group\" role=\"group\">\r\n                    <button v-if=\"this.isuser\" class=\"btn btn-outline-primary\" @click=\"requestBook(book.id)\">Request Book</button>\r\n                  </div>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport userMixins from '../mixins/userMixin';\r\nimport NavBar from '../components/NavBar.vue';\r\n\r\nexport default {\r\n  mixins: [userMixins],\r\n  data() {\r\n    return {\r\n      books: [],\r\n      user: {},\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getAllBooks();\r\n  },\r\n  methods: {\r\n    getAllBooks() {\r\n      fetch(\"http://127.0.0.1:5000/book\", {\r\n        method: 'GET',\r\n        headers: {\r\n          'Content-Type': 'application/json',\r\n          'Authorization': 'Bearer ' + localStorage.getItem('access_token'),\r\n        }\r\n      })\r\n      .then(response => {\r\n        if (!response.ok) {\r\n          throw new Error(`HTTP error: ${response.status}`);\r\n        }\r\n        return response.json();\r\n      })\r\n      .then(data => {\r\n        this.books = data.books;\r\n      })\r\n      .catch(error => {\r\n        console.error('Error:', error);\r\n      });\r\n    },\r\n    requestBook(book_id) {\r\n      fetch(`http://127.0.0.1:5000/bookrequest/${this.userid}/${book_id}`, {\r\n        method: 'POST',\r\n        headers: {\r\n          'Authorization': 'Bearer ' + localStorage.getItem('access_token'),\r\n          'Content-Type': 'application/json',\r\n        },\r\n      })\r\n      .then(response => {\r\n        if (!response.ok) {\r\n          throw new Error(\"Book request is not completed \");\r\n        }\r\n        return response.json();\r\n      })\r\n      .then(data => {\r\n        alert(data.message);\r\n      })\r\n      .catch(error => {\r\n        console.error(\"Error:\", error);\r\n        alert(\"An error occurred while requesting the book.\");\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.books-page {\r\n  font-family: 'Roboto', sans-serif;\r\n  background-color: #f7fafc;\r\n  min-height: 100vh;\r\n  padding: 2rem;\r\n}\r\n\r\nh2 {\r\n  font-size: 1.75rem;\r\n  color: #2d3748;\r\n  font-weight: 700;\r\n}\r\n\r\n.table {\r\n  border-radius: 8px;\r\n  background: #ffffff;\r\n}\r\n\r\n.table-bordered {\r\n  border: 1px solid #dee2e6;\r\n}\r\n\r\n.table thead {\r\n  background-color: #343a40;\r\n  color: #ffffff;\r\n}\r\n\r\n.table th {\r\n  font-weight: 600;\r\n}\r\n\r\n.table td {\r\n  font-size: 0.875rem;\r\n}\r\n\r\n.table-responsive {\r\n  width: 100%;\r\n}\r\n\r\n.btn-group .btn {\r\n  margin-right: 0.5rem;\r\n}\r\n\r\n.btn-outline-primary {\r\n  border-color: #3182ce;\r\n  color: #3182ce;\r\n}\r\n\r\n.btn-outline-primary:hover {\r\n  background-color: #3182ce;\r\n  color: #ffffff;\r\n}\r\n\r\na {\r\n  color: #3182ce;\r\n  text-decoration: none;\r\n}\r\n\r\na:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .table td, .table th {\r\n    font-size: 0.75rem;\r\n  }\r\n  \r\n  .btn-group .btn {\r\n    margin-bottom: 0.5rem;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAwCA,OAAOA,UAAS,MAAO,qBAAqB;AAC5C,OAAOC,MAAK,MAAO,0BAA0B;AAE7C,eAAe;EACbC,MAAM,EAAE,CAACF,UAAU,CAAC;EACpBG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,CAAC;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;EACpB,CAAC;EACDC,OAAO,EAAE;IACPD,WAAWA,CAAA,EAAG;MACZE,KAAK,CAAC,4BAA4B,EAAE;QAClCC,MAAM,EAAE,KAAK;QACbC,OAAO,EAAE;UACP,cAAc,EAAE,kBAAkB;UAClC,eAAe,EAAE,SAAQ,GAAIC,YAAY,CAACC,OAAO,CAAC,cAAc;QAClE;MACF,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACA,QAAQ,CAACC,EAAE,EAAE;UAChB,MAAM,IAAIC,KAAK,CAAC,eAAeF,QAAQ,CAACG,MAAM,EAAE,CAAC;QACnD;QACA,OAAOH,QAAQ,CAACI,IAAI,CAAC,CAAC;MACxB,CAAC,EACAL,IAAI,CAACX,IAAG,IAAK;QACZ,IAAI,CAACC,KAAI,GAAID,IAAI,CAACC,KAAK;MACzB,CAAC,EACAgB,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,QAAQ,EAAEA,KAAK,CAAC;MAChC,CAAC,CAAC;IACJ,CAAC;IACDE,WAAWA,CAACC,OAAO,EAAE;MACnBf,KAAK,CAAC,qCAAqC,IAAI,CAACgB,MAAM,IAAID,OAAO,EAAE,EAAE;QACnEd,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UACP,eAAe,EAAE,SAAQ,GAAIC,YAAY,CAACC,OAAO,CAAC,cAAc,CAAC;UACjE,cAAc,EAAE;QAClB;MACF,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACA,QAAQ,CAACC,EAAE,EAAE;UAChB,MAAM,IAAIC,KAAK,CAAC,gCAAgC,CAAC;QACnD;QACA,OAAOF,QAAQ,CAACI,IAAI,CAAC,CAAC;MACxB,CAAC,EACAL,IAAI,CAACX,IAAG,IAAK;QACZuB,KAAK,CAACvB,IAAI,CAACwB,OAAO,CAAC;MACrB,CAAC,EACAP,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,QAAQ,EAAEA,KAAK,CAAC;QAC9BK,KAAK,CAAC,8CAA8C,CAAC;MACvD,CAAC,CAAC;IACJ;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}