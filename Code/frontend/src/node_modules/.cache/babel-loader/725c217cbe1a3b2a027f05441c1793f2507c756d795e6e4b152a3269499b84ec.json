atus}`);\n        }\n        console.log('Book renewed');\n        this.$router.push('/book-request-user');\n      }).catch(error => {\n        console.log('Book could not be renewed');\n      });\n    }\n  }\n};","map":{"version":3,"names":["userMixins","NavBar","mixins","data","bookrequest","mounted","getBookByUser","methods","fetch","method","headers","localStorage","getItem","then","response","ok","Error","status","json","catch","error","console","returnBook","req_id","body","JSON","stringify","log","$router","push","renewBook"],"sources":["C:\\Users\\rchan\\Desktop\\uplagarize\\Code\\frontend\\src\\views\\UserView.vue"],"sourcesContent":["<template>\r\n  <div class=\"user-dashboard\">\r\n    <NavBar />\r\n    <div class=\"container mt-5\">\r\n      <h2>User Dashboard</h2>\r\n      <div class=\"books mt-5\">\r\n        <table class=\"table table-bordered table-hover\">\r\n          <thead class=\"table-dark\">\r\n            <tr>\r\n              <th>Book ID</th>\r\n              <th>Status</th>\r\n              <th>Request</th>\r\n              <th>Issue</th>\r\n              <th>Actions</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"bookreq in bookrequest\" :key=\"bookreq.book_id\">\r\n              <td>{{ bookreq.book_id }}</td>\r\n              <td>{{ bookreq.status }}</td>\r\n              <td>{{ bookreq.request }}</td>\r\n              <td>{{ bookreq.issue }}</td>\r\n              <td>\r\n                <div class=\"btn-group\" role=\"group\">\r\n                  <button v-if=\"isuser && bookreq.status !== 'request'\" class=\"btn btn-outline-primary\" @click=\"returnBook(bookreq.id)\">Return</button>\r\n                  <button v-if=\"isuser && bookreq.status !== 'request'\" class=\"btn btn-outline-primary\" @click=\"renewBook(bookreq.id)\">Renew</button>\r\n                  <button v-if=\"isuser && bookreq.status !== 'request'\" class=\"btn btn-outline-primary\" @click=\"\">Review</button>\r\n          