{"ast":null,"code":"import NavBar from \"@/components/NavBar.vue\";\nexport default {\n  components: {\n    NavBar\n  },\n  data() {\n    return {\n      email: \"\",\n      password: \"\"\n    };\n  },\n  methods: {\n    async login() {\n      const formData = {\n        email: this.email,\n        password: this.password\n      };\n      try {\n        const response = await this.postLoginData(formData);\n        const data = await response.json();\n        if (response.ok) {\n          alert(data.message);\n          localStorage.setItem(\"access_token\", data.access_token);\n          this.$router.push(\"/\");\n        } else {\n          alert(data.error);\n        }\n      } catch (error) {\n        console.error(\"Login error:\", error);\n        alert(\"An error occurred while attempting to login.\");\n      }\n    },\n    async postLoginData(formData) {\n      const response = await fetch(\"http://127.0.0.1:5000/login\", {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify(formData)\n      });\n      if (!response.ok) {\n        throw new Error(`HTTP error: ${response.status}`);\n      }\n      return response;\n    }\n  }\n};","map":{"version":3,"names":["NavBar","components","data","email","password","methods","login","formData","response","postLoginData","json","ok","alert","message","localStorage","setItem","access_token","$router","push","error","console","fetch","method","headers","body","JSON","stringify","Error","status"],"sources":["C:\\Users\\rchan\\Desktop\\uplagarize\\Code\\frontend\\src\\views\\LoginPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-page\">\r\n    <NavBar />\r\n    <div class=\"container my-5\">\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-md-6\">\r\n          <div class=\"card shadow-lg\">\r\n            <div class=\"card-body\">\r\n              <h2 class=\"card-title text-center mb-4\">Login</h2>\r\n              <form @submit.prevent=\"login\"> \r\n                <div class=\"mb-3\">\r\n                  <label for=\"email\" class=\"form-label\">Email</label>\r\n                  <input\r\n                    type=\"email\"\r\n                    id=\"email\"\r\n                    class=\"form-control\"\r\n                    v-model=\"email\"\r\n                    required\r\n                  />\r\n                </div>\r\n                \r\n                <div class=\"mb-3\">\r\n                  <label for=\"password\" class=\"form-label\">Password</label>\r\n                  <input\r\n                    type=\"password\"\r\n                    id=\"password\"\r\n                    class=\"form-control\"\r\n                    v-model=\"password\"\r\n                    required\r\n                  />\r\n                </div>\r\n                <button type=\"submit\" class=\"btn btn-primary w-100\">Login</button>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavBar from \"@/components/NavBar.vue\";\r\nexport default {\r\n  components: {\r\n    NavBar,\r\n  },\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      const formData = {\r\n        email: this.email,\r\n        password: this.password,\r\n      };\r\n\r\n      try {\r\n        const response = await this.postLoginData(formData);\r\n        const data = await response.json();\r\n\r\n        if (response.ok) {\r\n          alert(data.message);\r\n          localStorage.setItem(\"access_token\", data.access_token);\r\n          this.$router.push(\"/\");\r\n        } else {\r\n          alert(data.error);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Login error:\", error);\r\n        alert(\"An error occurred while attempting to login.\");\r\n      }\r\n    },\r\n    async postLoginData(formData) {\r\n      const response = await fetch(\"http://127.0.0.1:5000/login\", {\r\n        method: \"POST\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n        },\r\n        body: JSON.stringify(formData),\r\n      });\r\n\r\n      if (!response.ok) {\r\n        throw new Error(`HTTP error: ${response.status}`);\r\n      }\r\n\r\n      return response;\r\n    },\r\n  },\r\n};\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.login-page {\r\n  display: flex;\r\n  flex-direction: column;\r\n  min-height: 100vh;\r\n  background-color: #f7fafc;\r\n  font-family: 'Roboto', sans-serif;\r\n}\r\n\r\n.card {\r\n  border-radius: 10px;\r\n  background: #ffffff;\r\n}\r\n\r\n.card-body {\r\n  padding: 2rem;\r\n}\r\n\r\n.card-title {\r\n  font-size: 1.75rem;\r\n  color: #2d3748;\r\n  font-weight: 700;\r\n}\r\n\r\n.form-label {\r\n  font-weight: 600;\r\n  color: #4a5568;\r\n}\r\n\r\n.form-control {\r\n  border-radius: 5px;\r\n  font-size: 1rem;\r\n}\r\n\r\n.btn-primary {\r\n  background-color: #3182ce;\r\n  border-color: #3182ce;\r\n  font-size: 1rem;\r\n  padding: 0.75rem;\r\n  font-weight: 600;\r\n}\r\n\r\n.btn-primary:hover {\r\n  background-color: #2b6cb0;\r\n  border-color: #2b6cb0;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .card-body {\r\n    padding: 1.5rem;\r\n  }\r\n  \r\n  .card-title {\r\n    font-size: 1.5rem;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AA0CA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,MAAMC,QAAO,GAAI;QACfJ,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC;MAED,IAAI;QACF,MAAMI,QAAO,GAAI,MAAM,IAAI,CAACC,aAAa,CAACF,QAAQ,CAAC;QACnD,MAAML,IAAG,GAAI,MAAMM,QAAQ,CAACE,IAAI,CAAC,CAAC;QAElC,IAAIF,QAAQ,CAACG,EAAE,EAAE;UACfC,KAAK,CAACV,IAAI,CAACW,OAAO,CAAC;UACnBC,YAAY,CAACC,OAAO,CAAC,cAAc,EAAEb,IAAI,CAACc,YAAY,CAAC;UACvD,